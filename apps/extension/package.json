{
  "name": "Refreak - FACEIT Helper",
  "description": "Refreak enhances your experience on FACEIT and adds useful features",
  "private": true,
  "version": "1.0.8",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
